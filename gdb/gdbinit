set debuginfod enabled on
set confirm off
set print array on
set history filename ~/.gdb_history
set history save

define xxd
  dump binary memory /tmp/dump.bin $arg0 ((char *)$arg0)+$arg1
  shell xxd /tmp/dump.bin
end
document xxd
  Runs xxd on a memory ADDR and LENGTH

  xxd ADDR LENTH
end

macro define offsetof(t, f) &((t *) 0)->f
